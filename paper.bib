	Journal = {J.~Med.~Chem.},
	Keywords = {metabolism; metabolite; adme; random forest; interpretation},
	Pages = {3173--3184},
	Timescited = {0},
	Title = {Empirical Regioselectivity Models for Human Cytochromes {P450 3A4, 2D6, and 2C9}},
	Volume = {50},
	Year = {2007},
