	Keywords = {sar; qsar; optimization;},
	Mesh = {Drug Design; Models, Molecular; Phenylalanine; Protein Conformation; Quantitative Structure-Activity Relationship; Substrate Specificity; Thrombin; Trypsin; Trypsin Inhibitors},
	Month = {Oct},
