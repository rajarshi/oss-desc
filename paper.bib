	Doi = {10.1021/ci0498719},
	Issn = {1549-9596},
	Journal = {J.~Chem.~Inf.~Comput.~Sci.},
	Keywords = {similarity; molprint; circular; fingerprint; mddr},
	Number = {5},
	Pages = {1708--1718},
	Title = {Similarity Searching of Chemical Databases Using Atom Environment Descriptors (MOLPRINT 2D): Evaluation of Performance},
