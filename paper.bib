	Journal = {Can.~Res.},
	Keywords = {anticancer; NCI; DTP; modeling; model; platinum},
	Local-Url = {file://localhost/Users/rguha/Documents/articles/cans-res-vekris-356.pdf},
	Pages = {356--362},
	Title = {Molecular Determinants of the Cytotoxicity of Platinum Compounds: {The} Contribution of In Silico Research},
	Url = {http://cancerres.aacrjournals.org/cgi/content/full/64/1/356},
	Volume = {64},
