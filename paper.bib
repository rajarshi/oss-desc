	Keywords = {landscape; activity cliff;sali; sari; r-groups},
	Mesh = {Drug Delivery Systems; Ligands; Matrix Metalloproteinase 1; Models, Biological; Molecular Structure; Structure-Activity Relationship},
	Month = {Jul},
