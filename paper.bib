	Doi = {10.1021/ci034143r},
	Journal = {J.~Chem.~Inf.~Comput.~Sci.},
	Keywords = {qsar; genetic algorithm; spline; classification; benzodiazepine; estrogen; cox-2; dhfr},
	Local-Url = {file://localhost/Users/rguha/Documents/articles/ci034143r.pdf},
	Number = {1},
	Pages = {1906--1915},
