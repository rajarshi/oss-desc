@article{Ventura:2008aa,
	Abstract = {A QSAR/QSPR methodology was used to analyze a set of 173 hydrazides, a great part of which are isoniazid (INH) derivatives. Nineteen molecular descriptors of various types (physicochemical, steric, geometrical, and electronic) have been systematically tested through a careful application of MLR. The analysis revealed that the biological activity of these compounds against M. tuberculosis does not depend on lipophilicity, as measured by log P. Properties that account for the biological response of isoniazid and related compounds, consistent with a mechanism involving the formation of radical species, were identified. The role of substituents in the stabilization of the intermediate species that gives rise to the active agent, the acyl radical, is discussed. It is postulated that the activation of INH derivatives' prodrugs (hydrazines and hydrazones) occurs near the surface of M. tuberculosis.},
	Author = {Ventura, Cristina and Martins, Filomena},
	Date-Added = {2008-05-05 16:15:14 -0400},
