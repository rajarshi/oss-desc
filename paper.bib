	Journal = {J.~Med.~Chem.},
	Keywords = {aggregator; promiscuous;HTS; beta lactamase; dose concentration response; aggregation},
	Local-Url = {file://localhost/Users/rguha/Documents/articles/jm061317y.pdf},
	Number = {10},
