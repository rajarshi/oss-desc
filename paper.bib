	Journal = {Curr.~Cancer Drug Targets},
	Keywords = {rnai; tak1; nfkb; screen; hts;},
	Number = {8},
	Pages = {976--986},
	Title = {{RNAi} Screening Identifies {TAK1} as a Potential Target for the Enhanced Efficacy of Topoisomerase Inhibitors},
	Volume = {11},
	Year = {2011},
	Bdsk-Url-1 = {http://dx.doi.org/10.1002/cmdc.201100179}}

