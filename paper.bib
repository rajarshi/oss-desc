	Doi = {10.1016/j.jmgm.2006.11.005},
	Journal = {J.~Mol.~Graph.~Model.},
	Keywords = {mif; molecular interaction field; nAChRs; ligand; pyridyl ether; 3d; qsar; CoMFA; CoMSIA},
