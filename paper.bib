	Month = {Jun},
	Number = {12},
	Pages = {3739--3749},
	Pmid = {21616672},
	Pst = {ppublish},
	Title = {Extraction of Tacit Knowledge from Large {ADME} Data Sets via Pairwise Analysis},
	Volume = {19},
	Year = {2011},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.bmc.2011.05.003}}

