	Mesh = {Analgesics; Anti-Inflammatory Agents; Anti-Inflammatory Agents, Non-Steroidal; Drug Design; Models, Statistical; Quantitative Structure-Activity Relationship; Quantum Theory; Quinazolinones},
	Month = {Nov},
	Number = {14},
	Pages = {2445--2459},
	Pmid = {18452123},
	Pst = {ppublish},
	Title = {Desirability-based multiobjective optimization for global {QSAR} studies: {A}pplication to the design of novel {NSAID}s with improved analgesic, antiinflammatory, and ulcerogenic profiles},
