	Doi = {10.1055/s-2007-967138},
	Journal = {Planta Medica},
	Keywords = {malaria; angiogenesis; apoptosis; artemisinin; artesunate; cluster analysis; comparative genomic hybridization; microarrays; oxidative stess; pharmacogenomics},
