	Journal = {J.~Med.~Chem.},
	Keywords = {topomer},
	Owner = {rajarshi},
	Pages = {6777--6791},
	Timestamp = {2007.04.11},
	Title = {``{L}ead Hopping''. {V}alidation of Topomer Similarity as a Superior Predictor of Similar Biological Activities},
	Volume = {47},
	Year = {2004},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1021/jm049501b}}

