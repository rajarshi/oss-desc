	Journal = {Chemosphere},
	Keywords = {mixture toxicity; quantum chemical parameters; ab initio; MO; qsar; molecular structure},
	Pages = {396--401},
	Title = {Computer-Based {QSAR}s for Predicting Mixture Toxicity of Benzene and Its Derivatives},
	Volume = {67},
	Year = {2007},
