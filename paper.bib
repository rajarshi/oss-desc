	Journal = {Biochem.~Pharamcol.},
	Keywords = {NCI; DTP; anticancer; antitumor quinones; NQO1; redox cycling; hydroxyl radicals; apoptosis; cytochrome c release; BAD protein},
	Local-Url = {file://localhost/Users/rguha/Documents/articles/tudor-biochempharmacol-2003-65-1061.pdf},
