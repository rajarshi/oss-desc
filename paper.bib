	Journal = {IEEE Trans.~Inf.~Theory},
	Keywords = {similarity metric; dissimilarity distance; Kolmogorov complexity; language tree construction; normalized compression distance; normalized information distance; parameter-free data mining; phylogeny in bioinformatics; universal similarity metric},
