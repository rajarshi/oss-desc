	Keywords = {profile; assay; pubchem; qsar; sar;},
	Mesh = {Artificial Intelligence; Cytochrome P-450 CYP1A2; Enzyme Inhibitors; Humans; Molecular Conformation; Quantitative Structure-Activity Relationship},
	Month = {Jun},
	Number = {6},
	Pages = {1271--1280},
	Pmid = {21598906},
	Title = {A comparison of different {QSAR} approaches to modeling {CYP450 1A2} inhibition},
	Volume = {51},
	Year = {2011},
