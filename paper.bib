	Journal = {J.~Med.~Chem.},
	Journal-Full = {Journal of medicinal chemistry},
	Mesh = {Adenosine A2 Receptor Antagonists; Animals; Cell Line; Cricetinae; Cricetulus; Humans; Pyrazoles; Radioligand Assay; Theophylline; Xanthines},
	Month = {Jun},
