	Keywords = {lipase; diabetes; hsl; pharmacophore; catalyst; hypogen; qsar; obesity; virtual screen},
	Number = {20},
	Pages = {6478--6494},
	Title = {Combining Ligand-Based Pharmacophore Modeling, Quantitative Structure\&{\#}x2212;Activity Relationship Analysis and in Silico Screening for the Discovery of New Potent Hormone Sensitive Lipase Inhibitors},
	Url = {http://pubs3.acs.org/acs/journals/doilookup?in_doi=10.1021/jm800718k},
