	Keywords = {metabolism; cytochrome; selectivity; reactivity; docking; autodock},
	Number = {5},
	Pages = {1074--1080},
	Rating = {2},
	Title = {Regioselectivity Prediction of {CYP1A2}-Mediated Phase {I} Metabolism},
	Url = {http://pubs3.acs.org/acs/journals/doilookup?in_doi=10.1021/ci800001m},
